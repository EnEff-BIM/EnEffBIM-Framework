<%page args="jObj"/>
  final constant FluidConstants
  ${jObj['NAME'].replace('-','')}FluidConstants(
    casRegistryNumber="${jObj['CAS']}",
    iupacName="${jObj['NAME']}" "full name",
    structureFormula="",
    chemicalFormula="${jObj['FORMULA'].translate({ord(i):None for i in '-_{}'})}",
    molarMass=${jObj['EOS'][0]['molar_mass']},
    hasCriticalData=true,
       criticalTemperature=${jObj['EOS'][0]['STATES']['reducing']['T']},
       criticalPressure=${jObj['EOS'][0]['STATES']['reducing']['p']},
       criticalMolarVolume=1/${jObj['EOS'][0]['STATES']['reducing']['rhomolar']},
       HCRIT0=${jObj['STATES']['critical']['hmolar']}/${jObj['EOS'][0]['molar_mass']},
       SCRIT0=${jObj['STATES']['critical']['smolar']}/${jObj['EOS'][0]['molar_mass']},
    hasIdealGasHeatCapacity=false,
    hasDipoleMoment=false,
    hasFundamentalEquation=true,
    hasLiquidHeatCapacity=true,
    hasSolidHeatCapacity=false,
    hasAccurateViscosityData=true,
    hasAccurateConductivityData=true,
    hasVapourPressureCurve=true,
    hasAcentricFactor=true,
       acentricFactor=${jObj['EOS'][0]['acentric']},
    triplePointTemperature=${jObj['STATES']['triple_liquid']['T']},
    triplePointPressure=${jObj['STATES']['triple_liquid']['p']},
    normalBoilingPoint="",
    meltingPoint="") "Fluid Constants";
